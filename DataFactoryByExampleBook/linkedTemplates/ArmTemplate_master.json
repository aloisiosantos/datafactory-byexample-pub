{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFactoryByExampleBook"
		},
		"AdfByExample_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AdfByExample'"
		},
		"AzureDataFactoryByExamp_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AzureDataFactoryByExamp.vault.azure.net/"
		},
		"adfbyexamplebook_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://adfbyexamplebook.blob.core.windows.net"
		},
		"adfbyexamplebook_keyvault_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "StorageAccount-ConnectionString"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFactoryByExampleBook_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdfByExample_connectionString": {
						"value": "[parameters('AdfByExample_connectionString')]"
					},
					"AzureDataFactoryByExamp_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureDataFactoryByExamp_properties_typeProperties_baseUrl')]"
					},
					"adfbyexamplebook_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('adfbyexamplebook_properties_typeProperties_serviceEndpoint')]"
					},
					"adfbyexamplebook_keyvault_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('adfbyexamplebook_keyvault_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}